pipeline {
    agent any
    stages {
        stage ('init') {
            steps {
               echo "start jenkins"
            }
        }
	stage ('Execution Selenium Script from Local Machine') {
	    steps {
	       bat label: '', script: '''cd "C:\\CICD_Jenkins"
	        "C:\\Program Files (x86)\\ojdkbuild\\java-1.8.0-openjdk-1.8.0.222-2\\jre\\bin\\java" -jar SeleniumWebPage.jar'''
	    }
        }
        stage ('Report') 
	    {
            steps {
                 step([$class: 'XrayImportBuilder', endpointName: '/junit', importFilePath: 'xrayReport', importToSameExecution: 'true', projectKey: 'COV', serverInstance: 'SERVER-7f86a19a-07b8-4fdb-96c8-abb3bbead4c0'])
                }
           }
       }
   }
